<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/linearLayout10"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/appbg"
	tools:context=".LawyerAddNewEvent">

	<TextView
		android:id="@+id/textView24"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="100dp"
		android:text="Add New Event"
		android:textSize="20sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.498"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/editTextText8"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp"
		android:ems="10"
		android:inputType="text"
		android:text="Name"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.502"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView24" />

	<EditText
		android:id="@+id/editTextText9"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="28dp"
		android:ems="10"
		android:inputType="text"
		android:text="Name"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.502"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/editTextText8" />

	<EditText
		android:id="@+id/editTextDate"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="56dp"
		android:ems="10"
		android:inputType="date"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.502"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/editTextText9" />

	<EditText
		android:id="@+id/editTextText10"
		android:layout_width="133dp"
		android:layout_height="47dp"
		android:layout_marginStart="40dp"
		android:ems="10"
		android:inputType="text"
		android:text="Name"
		app:layout_constraintEnd_toStartOf="@+id/editTextText11"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		tools:layout_editor_absoluteY="429dp" />

	<EditText
		android:id="@+id/editTextText11"
		android:layout_width="133dp"
		android:layout_height="47dp"
		android:layout_marginEnd="40dp"
		android:ems="10"
		android:inputType="text"
		android:text="Name"
		app:layout_constraintEnd_toEndOf="parent"
		tools:layout_editor_absoluteY="429dp" />

	<Button
		android:id="@+id/button6"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="128dp"
		android:text="Create Event"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.498"
		app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>